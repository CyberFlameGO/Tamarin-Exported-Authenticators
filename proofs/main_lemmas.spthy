     (Recv( actor, ms, role, <del2, pk(ltkD2)> ) @ #k)) ⇒
    (((∃ peer role2 #h #i.
        ((((Owns( peer, ms, role2, <del, ltkD> ) @ #h) ∧ (#h < #j)) ∧
          (Owns( peer, ms, role2, <del2, ltkD2> ) @ #i)) ∧
